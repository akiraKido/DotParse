<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>Revn.DotParse</AssemblyName>
    <RootNamespace>Revn.DotParse</RootNamespace>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Description>Simple Parser Combinator for .NET Standard</Description>
    <Version>0.0.1</Version>
    <Copyright></Copyright>
    <PackageLicenseUrl>https://github.com/revn-project/DotParse/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/revn-project</PackageProjectUrl>
    <RepositoryUrl>https://github.com/revn-project/DotParse</RepositoryUrl>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <TreatWarningsAsErrors>False</TreatWarningsAsErrors>
    <TreatSpecificWarningsAsErrors />
    <LangVersion>7</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.ValueTuple" Version="4.3.1" />
  </ItemGroup>

</Project>